{
	"version": "0.1",
	"library_root": "~/Music",
	"delete_removed": false,
	"delete_updated": false,
	"opus": {
		"enabled": false,
		"bitrate": 96,
		"embed_cover": false,
		"replaygain": {
			"mode": "replaygain",
			"clipping_policy": "none"
		}
